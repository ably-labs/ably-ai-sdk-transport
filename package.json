{
  "name": "@ably/ai-sdk-transport",
  "version": "0.1.0",
  "description": "Ably transport for the Vercel AI SDK â€” replaces HTTP SSE with Ably pub/sub for streaming AI responses",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'"
  },
  "peerDependencies": {
    "ably": "^2.6.0",
    "ai": "^6.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.0",
    "ably": "^2.6.0",
    "ai": "^6.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^3.0.0"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ably/ai-sdk-transport.git"
  },
  "keywords": [
    "ably",
    "ai-sdk",
    "vercel",
    "transport",
    "streaming",
    "pubsub",
    "realtime"
  ]
}
